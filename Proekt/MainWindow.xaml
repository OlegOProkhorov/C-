<Window x:Class="Praktika_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktika_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Вычисление корней" FontSize="30" HorizontalAlignment="Right" VerticalAlignment="Center"></TextBlock>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFraim_ContentRendered" Grid.Row="1" Name="MainFraim" ></Frame>
        <Button Content="Линейное уравнение" HorizontalAlignment="Left" Margin="390,59.8,0,30.4" Grid.Row="1" Width="394">
        </Button>
        <Button Content="Квадратное уравнение" HorizontalAlignment="Left" Margin="10,59.8,0,30.4" Grid.Row="1" Width="350">
        </Button>



    </Grid>
</Window>
